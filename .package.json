{
  "name": "angular-material",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod",
    "serve": "ng serve --aot",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "cypress:open",
    "cypress:open": "concurrently \"ng serve\" \"wait-on http-get://localhost:4300 && cypress open\"",
    "cypress:run": "cypress run --spec cypress/integration/features/**/* > reports/cypress-report.log",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng run Angular-Material:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "prestyleguide": "ng build --extract-css true --output-path kss/dist-style",
    "styleguide": "rimraf styleguide && kss --config kss/kss-config.json",
    "poststyleguide": "cp -r src/assets/ kss/dist-style/styles.css  kss/kss-styles.css kss/index.php styleguide/ && rimraf kss/dist-style",
    "lighthouse": "concurrently \"ng serve\" \"wait-on http-get://localhost:4300 && lighthouse http://localhost:4300 --view --output-path ./reports/lighthouse-report.html\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.2",
    "@angular/cdk": "^7.0.2",
    "@angular/common": "^7.0.2",
    "@angular/compiler": "^7.0.2",
    "@angular/core": "^7.0.2",
    "@angular/forms": "^7.0.2",
    "@angular/http": "^7.0.2",
    "@angular/material": "^7.0.2",
    "@angular/platform-browser": "^7.0.2",
    "@angular/platform-browser-dynamic": "^7.0.2",
    "@angular/platform-server": "^7.0.2",
    "@angular/pwa": "^0.10.4",
    "@angular/router": "^7.0.2",
    "@angular/service-worker": "^7.0.2",
    "@ngrx/effects": "^6.1.2",
    "@ngrx/entity": "^6.1.2",
    "@ngrx/router-store": "^6.1.2",
    "@ngrx/store": "^6.1.2",
    "@ngrx/store-devtools": "^6.1.2",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
    "core-js": "^2.5.7",
    "express": "^4.16.4",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "ngx-pagination": "^3.2.1",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular/language-service": "^7.0.2",
    "@bahmutov/add-typescript-to-cypress": "^2.0.0",
    "@compodoc/compodoc": "^1.1.6",
    "@ngrx/schematics": "^6.1.2",
    "@types/express": "^4.16.0",
    "@types/jasmine": "~2.8.9",
    "@types/jasminewd2": "~2.0.5",
    "codelyzer": "~4.5.0",
    "concurrently": "^4.0.1",
    "cucumber": "^5.0.2",
    "cypress": "^3.1.1",
    "cypress-cucumber-preprocessor": "^1.6.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.4.0",
    "kss": "^3.0.0-beta.23",
    "michelangelo": "^0.8.0",
    "ngrx-store-freeze": "^0.2.4",
    "protractor": "~5.4.1",
    "ts-loader": "^5.3.0",
    "ts-node": "~7.0.1",
    "tsify": "^4.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.3",
    "wait-on": "^3.2.0",
    "webpack-cli": "^3.1.2"
  }
}
